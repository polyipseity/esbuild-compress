  },
  "dependencies": {
    "import-meta-resolve": "^4.1.0",
    "lodash-es": "^4.17.21",
    "lz-string": "^1.5.0",
    "p-lazy": "^5.0.0"
  },
  "description": "Compression plugin for esbuild.",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.1.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.2.0",
    "happy-dom": "^20.6.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.20.0",
    "prettier": "^3.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "files": [
    "src/index.js"
  ],
  "funding": [
    {
      "type": "Buy Me a Coffee",
      "url": "https://buymeacoffee.com/polyipseity"
    },
    {
      "type": "GitHub Sponsors",
      "url": "https://github.com/sponsors/polyipseity"
    }
  ],
  "homepage": "https://github.com/polyipseity/esbuild-compress#readme",
  "keywords": [
    "compress",
    "compression",
    "esbuild",
    "esbuild-plugin",
    "plugin"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "name": "esbuild-compress",
  "overrides": {},
  "peerDependencies": {
    "esbuild": ">=0.17.0"
  },
  "pnpm": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polyipseity/esbuild-compress.git"
  },
  "scripts": {
    "build": "pnpm run check && pnpm run build:force",
    "build:force": "node -e \"console.log('No build step; package is ready to import (src/index.js)')\"",
    "check": "pnpm run check:eslint && pnpm run check:md && pnpm run check:prettier",
    "check:eslint": "eslint --cache --max-warnings=0",
    "check:md": "markdownlint-cli2",
    "check:prettier": "prettier --cache --check \"**/*.{astro,cjs,css,csv,gql,graphql,hbs,html,js,jsx,json,json5,jsonc,jsonl,less,mjs,pcss,sass,scss,svelte,styl,ts,tsx,vue,xml,yaml,yml}\"",
    "commitlint": "commitlint --from=origin/main --to=HEAD",
    "format": "pnpm run format:eslint && pnpm run format:md && pnpm run format:prettier",
    "format:eslint": "eslint --cache --fix",
    "format:md": "markdownlint-cli2 --fix",
    "format:prettier": "prettier --cache --write \"**/*.{astro,cjs,css,csv,gql,graphql,hbs,html,js,jsx,json,json5,jsonc,jsonl,less,mjs,pcss,sass,scss,svelte,styl,ts,tsx,vue,xml,yaml,yml}\"",
    "prepare": "husky",
    "test": "pnpm run test:vitest",
    "test:vitest": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "sideEffects": false,
  "type": "module",
  "version": "2.1.0"
}
